<rng:grammar xmlns:rng="http://relaxng.org/ns/structure/1.0"
             xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" ns=""
             datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <rng:start combine="choice">
        <rng:ref name="response"/>
    </rng:start>
    <rng:define name="response">
        <rng:element name="response">
            <rng:element name="meta">
                <rng:element name="next">
                    <rng:data type="anyURI">
                        <a:documentation>
                            The API URL for the next page (or empty string for the last page)
                        </a:documentation>
                    </rng:data>
                </rng:element>
                <rng:element name="previous">
                    <rng:data type="anyURI">
                        <a:documentation>
                            The API URL for the previous page (or empty string for the first page)
                        </a:documentation>
                    </rng:data>
                </rng:element>
                <rng:element name="total_count">
                    <rng:data type="short">
                        <a:documentation>
                            How many productions are there in total?
                        </a:documentation>
                    </rng:data>
                </rng:element>
                <rng:element name="items_per_page">
                    <rng:data type="byte">
                        <a:documentation>
                            What is the maximal amount of productions per page?
                        </a:documentation>
                    </rng:data>
                </rng:element>
            </rng:element>
            <rng:element name="productions">
                <rng:zeroOrMore>
                    <rng:element name="production">
                        <rng:element name="id">
                            <rng:data type="byte">
                                <a:documentation>
                                    Unique production ID on your website
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="creation_date">
                            <rng:data type="dateTime">
                                <a:documentation>
                                    Production creation timestamp in ISO 8601 format
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="modified_date">
                            <rng:data type="dateTime">
                                <a:documentation>
                                    Production modification timestamp in ISO 8601 format
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="status">
                            <rng:data type="string">
                                <a:documentation>
                                    Publishing status, one of: "draft", "published", "not_listed", "expired", "trashed"
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="prefix_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Title prefix in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="prefix_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Title prefix in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="title_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Title in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="title_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Title in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="subtitle_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Subtitle (Unterüberschrift) in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="subtitle_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Subtitle (Unterüberschrift) in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="original_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Original title in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="original_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Original title in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="website_de">
                            <rng:data type="anyURI">
                                <a:documentation>
                                    A link to the your website page about this production in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="website_en">
                            <rng:data type="anyURI">
                                <a:documentation>
                                    A link to the your website page about this production in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="description_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text description in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="description_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text description in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="teaser_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text teaser in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="teaser_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text teaser in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="work_info_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text work info in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="work_info_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text work info in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="contents_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text contents in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="contents_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text contents in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="press_text_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text press text in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="press_text_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text press text in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="credits_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text credits in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="credits_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text credits in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="concert_program_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text concert program in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="concert_program_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text concert program in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="supporting_program_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text supporting program in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="supporting_program_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text supporting program in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="remarks_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text remarks in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="remarks_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text remarks in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="duration_text_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about duration in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="duration_text_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about duration in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="subtitles_text_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about subtitles in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="subtitles_text_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about subtitles in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="age_text_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about the age of the audience in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="age_text_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Plain-text information about the age of the audience in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="ensembles">
                            <rng:data type="string">
                                <a:documentation>
                                    Ensemble or ensembles playing in this production
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="organizers">
                            <rng:data type="string">
                                <a:documentation>
                                    Organizer or organizers of this production
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="in_cooperation_with">
                            <rng:data type="string">
                                <a:documentation>
                                    Cooperator or cooperators
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="free_entrance">
                            <rng:data type="string">
                                <a:documentation>
                                    Is the entrance free? One of "True" or "False"
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="price_from">
                            <rng:data type="float">
                                <a:documentation>
                                    Price from in Euros (decimal, no currency sign included) Price from in Euros
                                    (decimal, no currency sign included)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="price_till">
                            <rng:data type="float">
                                <a:documentation>
                                    Price till in Euros (decimal, no currency sign included)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="tickets_website">
                            <rng:data type="string">
                                <a:documentation>
                                    The URL of a website page where you can buy tickets to this production
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="price_information_de">
                            <rng:data type="string">
                                <a:documentation>
                                    Additional plain-text information about prices in German
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="price_information_en">
                            <rng:data type="string">
                                <a:documentation>
                                    Additional plain-text information about prices in English
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="age_from">
                            <rng:data type="string">
                                <a:documentation>
                                    Audience age from (integer)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="age_till">
                            <rng:data type="string">
                                <a:documentation>
                                    Audience age till (integer)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="edu_offer_website">
                            <rng:data type="string">
                                <a:documentation>
                                    The URL of a website page with educational offer
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="in_program_of">
                            <a:documentation>
                                Theaters organizing this production. Check the list of available IDs at the
                                specification
                            </a:documentation>
                            <rng:zeroOrMore>
                                <rng:element name="location_id">
                                    <rng:data type="byte"/>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="play_locations">
                            <a:documentation>
                                Theaters where this production takes place. Check the list of available IDs at the
                                specification
                            </a:documentation>
                            <rng:zeroOrMore>
                                <rng:element name="location_id">
                                    <rng:data type="byte"/>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="play_stages">
                            <a:documentation>
                                Stages where this production takes place. Check the list of available IDs at the
                                specification
                            </a:documentation>
                            <rng:zeroOrMore>
                                <rng:element name="stage_id">
                                    <rng:data type="byte"/>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="location_title">
                            <rng:data type="string">
                                <a:documentation>
                                    Location title (if play_locations is empty)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="street_address">
                            <rng:data type="string">
                                <a:documentation>
                                    Street address (first line) of the location (if play_locations is empty)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="street_address2">
                            <rng:data type="string">
                                <a:documentation>
                                    Street address (second line) of the location (if play_locations is empty)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="postal_code">
                            <rng:data type="string">
                                <a:documentation>
                                    Postal code of the location (if play_locations is empty)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="city">
                            <rng:data type="string">
                                <a:documentation>
                                    City of the location (if play_locations is empty)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="latitude">
                            <rng:data type="float">
                                <a:documentation>
                                    Latitude of the location (if play_locations is empty, decimal format)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="longitude">
                            <rng:data type="float">
                                <a:documentation>
                                    Longitude of the location (if play_locations is empty, decimal format)
                                </a:documentation>
                            </rng:data>
                        </rng:element>
                        <rng:element name="categories">
                            <a:documentation>
                                Categories. Check the list of available IDs at the specification
                            </a:documentation>
                            <rng:zeroOrMore>
                                <rng:element name="category_id">
                                    <rng:data type="byte"/>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="characteristics">
                            <a:documentation>
                                Characteristics. Check the list of available IDs at the specification
                            </a:documentation>
                            <rng:zeroOrMore>
                                <rng:element name="characteristic_id">
                                    <rng:data type="byte"/>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="leaders">
                            <rng:zeroOrMore>
                                <rng:element name="leader">
                                    <rng:element name="prefix_id">
                                        <rng:data type="string">
                                            <a:documentation>
                                                One of: "mr", "ms", "dr", "prof", "mr-dr", "ms-dr", "mr-prof",
                                                "ms-prof", "mr-prof-dr", "ms-prof-dr"
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="first_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                First name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="last_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Last name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="function_de">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Description in German what this leader is doing for the production
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="function_en">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Description in English what this leader is doing for the production
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="authors">
                            <rng:zeroOrMore>
                                <rng:element name="author">
                                    <rng:element name="prefix_id">
                                        <rng:data type="string">
                                            <a:documentation>
                                                One of: "mr", "ms", "dr", "prof", "mr-dr", "ms-dr", "mr-prof",
                                                "ms-prof", "mr-prof-dr", "ms-prof-dr"
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="first_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                First name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="last_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Last name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="type_id">
                                        <rng:data type="string">
                                            <a:documentation>
                                                What did this author do in the production? One of: "komponist", "autor",
                                                "uebersetzer"
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="participants">
                            <rng:zeroOrMore>
                                <rng:element name="participant">
                                    <rng:element name="prefix_id">
                                        <rng:data type="string">
                                            <a:documentation>
                                                One of: "mr", "ms", "dr", "prof", "mr-dr", "ms-dr", "mr-prof",
                                                "ms-prof", "mr-prof-dr", "ms-prof-dr"
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="first_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                First name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="last_name">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Last name of the person
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="type_id">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Involvement type ID. Check the list of available IDs at the
                                                specification
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="role_de">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Role in German
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="role_en">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Role in English
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="instrument_de">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Instrument in German
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="instrument_en">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Instrument in English
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="videos">
                            <rng:zeroOrMore>
                                <rng:element name="video">
                                    <rng:element name="creation_date">
                                        <rng:data type="dateTime">
                                            <a:documentation>
                                                Creation timestamp in ISO 8601
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="modified_date">
                                        <rng:data type="dateTime">
                                            <a:documentation>
                                                Modification timestamp in ISO 8601 format
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="title_de">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Title in German
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="title_en">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Title in English
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="embed">
                                        <rng:data type="string">
                                            <a:documentation>
                                                HTML embed code
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="sort_order">
                                        <rng:data type="byte">
                                            <a:documentation>
                                                Sort order of the video (integer)
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                </rng:element>
                            </rng:zeroOrMore>
                        </rng:element>
                        <rng:element name="live_streams">
                            <rng:zeroOrMore>
                                <rng:element name="live_stream">
                                    <rng:element name="creation_date">
                                        <rng:data type="dateTime">
                                            <a:documentation>
                                                Creation timestamp in ISO 8601
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="modified_date">
                                        <rng:data type="dateTime">
                                            <a:documentation>
                                                Modification timestamp in ISO 8601 format
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="title_de">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Title in German
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="title_en">
                                        <rng:data type="string">
                                            <a:documentation>
                                                Title in English
                                            </a:documentation>
                                        </rng:data>
                                    </rng:element>
                                    <rng:element name="embed">
