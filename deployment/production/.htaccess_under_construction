AddHandler wsgi-script .wsgi
DirectoryIndex index.html index.php
RewriteEngine On

RewriteBase /
RewriteCond %{REMOTE_HOST} !^217\.92\.175\.81

RewriteCond %{REQUEST_URI} !/temporarily-offline\.html$
RewriteCond %{REQUEST_URI} !/kb-logo\.png$
RewriteCond %{REQUEST_URI} !^/media
RewriteCond %{REQUEST_URI} !^/jetson-media
RewriteCond %{REQUEST_URI} !^/static
RewriteRule .* /temporarily-offline.html [R=302,L]

RewriteCond %{REQUEST_URI} !/temporarily-offline\.html$
RewriteCond %{REQUEST_URI} !/kb-logo\.png$
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}/index.html !-f
RewriteCond %{REQUEST_FILENAME}/index.php !-f
RewriteCond %{REQUEST_URI} !^/media
RewriteCond %{REQUEST_URI} !^/jetson-media
RewriteCond %{REQUEST_URI} !^/static
RewriteRule ^(.*)$ /my.wsgi/$1 [QSA,L]
